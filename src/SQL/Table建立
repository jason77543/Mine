CREATE TABLE LETTERTYPE (
 LETTERTYPENO              		NUMBER(8) ,
 LETTERTYPENAME                	VARCHAR2(100) NOT NULL,
 LETTERTYPETEXT                	CLOB NOT NULL,
 CONSTRAINT lettertype_pk PRIMARY KEY(LETTERTYPENO) );
 
CREATE SEQUENCE LETTERTYPE_SEQ
INCREMENT BY 1
START WITH 30001
NOMAXVALUE
Nocycle
NOCACHE;
 
 
 INSERT INTO LETTERTYPE VALUES (LETTERTYPE_SEQ.NEXTVAL, '約會成功', '恭喜你約會已經成功了');
 INSERT INTO LETTERTYPE VALUES (LETTERTYPE_SEQ.NEXTVAL, '約會失敗', '恭喜你約會已經失敗了');
 INSERT INTO LETTERTYPE VALUES (LETTERTYPE_SEQ.NEXTVAL, '被申訴', '恭喜你被申訴了');
 Insert Into Lettertype Values (Lettertype_Seq.Nextval, '申訴', '恭喜你申訴已經成功了');
 INSERT INTO LETTERTYPE VALUES (LETTERTYPE_SEQ.NEXTVAL, '檢舉', '恭喜你檢舉已經成功了');




CREATE TABLE LETTER (
 LETTERNO              			NUMBER(8) ,
 LETTERTYPENO                	NUMBER(8) NOT NULL,
 MEMNO                			NUMBER(10) NOT NULL,
 LETTERTIME						TIMESTAMP NOT NULL,
 LETTERSTATE					NUMBER(2) NOT NULL,
 LETTERTAG						NUMBER(2) NOT NULL,
 LETTERTEXT						CLOB,
 CONSTRAINT letter_pk PRIMARY KEY(LETTERNO),
 CONSTRAINT letter_lettertypeno_fk FOREIGN KEY(LETTERTYPENO) REFERENCES LETTERTYPE(LETTERTYPENO),
 CONSTRAINT letter_memno_fk FOREIGN KEY(MEMNO) REFERENCES MEMBER(MEMNO)
 );
 
 CREATE SEQUENCE LETTER_SEQ
INCREMENT BY 1
START WITH 40001
NOMAXVALUE
Nocycle
NOCACHE;
 
 INSERT INTO LETTER VALUES (LETTER_SEQ.NEXTVAL, 30001,5001,systimestamp,0,1, '恭喜你約會');
 INSERT INTO LETTER VALUES (LETTER_SEQ.NEXTVAL, 30002,5002,systimestamp,1,0, '恭喜你約會失敗');
 INSERT INTO LETTER VALUES (LETTER_SEQ.NEXTVAL, 30003,5004,systimestamp,0,1, '恭喜你被申訴');
 Insert Into Letter Values (Letter_Seq.Nextval, 30004,5003,Systimestamp,1,0, '恭喜你申訴成功了');
 INSERT INTO LETTER VALUES (LETTER_SEQ.NEXTVAL, 30005,5005,systimestamp,0,1, '恭喜你被檢舉已經成功了');
 
 




CREATE TABLE SUBMEM(
 ACTSUBMEMNO              	NUMBER(10) ,
 BESUBMEMNO                	NUMBER(10) ,
 SUBSTATE                	NUMBER(1) NOT NULL,
 Constraint Submem_Pk Primary Key(Actsubmemno,Besubmemno),
Constraint Submem_Besub_Fk Foreign Key(Besubmemno) References MEMBER(MEMNO), 
Constraint SUBMEM_ACTSUB_Fk Foreign Key(ACTSUBMEMNO) References MEMBER(MEMNO));


 INSERT INTO SUBMEM VALUES (5001, 5002, 0);
 INSERT INTO SUBMEM VALUES (5001, 5003, 0);
 INSERT INTO SUBMEM VALUES (5001, 5004, 0);
 INSERT INTO SUBMEM VALUES (5002, 5001, 0);
 INSERT INTO SUBMEM VALUES (5002, 5005, 0);
 Insert Into Submem Values (5004, 5003, 0);
 INSERT INTO SUBMEM VALUES (5004, 5002, 0);
 
 
 Create Table Appreprec (
 MEMNO              	NUMBER(10) ,
 RECDATE                DATE ,
 CONSTRAINT appreprec_pk PRIMARY KEY(memno,recdate),
 CONSTRAINT appreprec_fk FOREIGN KEY(memno) REFERENCES MEMBER(MEMNO) );
 
 INSERT INTO APPREPREC VALUES (5001, TO_DATE('2015-04-23','YYYY-MM-DD'));
 INSERT INTO APPREPREC VALUES (5001, TO_DATE('2016-07-23','YYYY-MM-DD'));
 INSERT INTO APPREPREC VALUES (5001, TO_DATE('2017-09-28','YYYY-MM-DD'));
 INSERT INTO APPREPREC VALUES (5002, TO_DATE('2012-04-30','YYYY-MM-DD'));
 Insert Into Appreprec Values (5002, To_Date('2011-08-25','YYYY-MM-DD'));



commit;

